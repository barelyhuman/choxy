{
  "name": "@barelyhuman/choxy",
  "version": "0.0.3-beta.2",
  "repository": "git@github.com:barelyhuman/choxy.git",
  "license": "MIT",
  "author": "Reaper <ahoy@barelyhuman.dev>",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks.mjs",
      "require": "./dist/hooks.js",
      "types": "./dist/hooks.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nr build:hooks; nr build:cacher",
    "build:hooks": "tsup src/hooks.ts --format cjs,esm --dts --target=node12.22",
    "build:cacher": "tsup src/index.ts --format cjs,esm --dts --target=node12.22",
    "dev": "pnpm build -- --watch",
    "fix": "eslint --fix .",
    "test": "echo 'true'",
    "next": "bumpp --commit --tag --push"
  },
  "prettier": "@barelyhuman/prettier-config",
  "dependencies": {
    "@types/react": "^18.0.14"
  },
  "devDependencies": {
    "@antfu/ni": "^0.16.2",
    "@barelyhuman/prettier-config": "^1.0.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "packageManager": "pnpm@6.32.10"
}
